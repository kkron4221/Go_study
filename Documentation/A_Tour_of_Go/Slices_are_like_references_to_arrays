package main

import "fmt"

func main(){
    names := [4]string{
        "John",
        "Paul",
        "George",
        "Ringo",
    }
    fmt.Println(names)
    
    // []'s second num does not include, so var a and var b output two arguments.
    a := names[0:2]
    b := names[1:3]
    fmt.Println(a, b)
    
    b[0] = "XXX"
    fmt.Println(a, b)
    fmt.Println(names)
}
